# Example environment configuration
# Copy this file to .env and fill in your values

# Flask environment
FLASK_ENV=development

# Database configuration
DATABASE_URL=postgresql+psycopg2://postgres:password@localhost:5433/findameetingspot_dev

# Security
ENCRYPTION_KEY=generate-a-secure-key-using-fernet-generate-key
JWT_SECRET_KEY=generate-a-secure-random-string

# Frontend URL
FRONTEND_URL=http://localhost:3000

# OAuth Configuration
LINKEDIN_CLIENT_ID=your-linkedin-client-id
LINKEDIN_CLIENT_SECRET=your-linkedin-client-secret
LINKEDIN_CALLBACK_URL=http://localhost:8000/api/v1/auth/linkedin/callback

# Optional services
GOOGLE_MAPS_API_KEY=your-google-maps-api-key
MAILGUN_API_KEY=your-mailgun-api-key
MAILGUN_DOMAIN=your-mailgun-domain
