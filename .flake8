[flake8]
max-line-length = 88
extend-ignore = E203,W503,E402,E722
exclude = .git,__pycache__,build,dist
per-file-ignores =
    __init__.py: F401
    migrations/*: F401
    scripts/*: F401
    utils/*: F401
    app/*: F401
    config.py: F401
    development.py: F401
    wsgi.py: F401
    check_tables.py: F401
    test_connection.py: F401
    fix_linting.py: F401
    tests/*: F401
max-complexity = 15
